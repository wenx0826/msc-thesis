    <data type="string" rngui:readonly="true"/>
  </attribute>
  <element name="parameters" rngui:header="Properties">
    <element name="label" rngui:label="Label">
      <data type="string" rngui:label="Short description of the task"/>
    </element>
    <element name="color" rngui:label="Task Color">
      <choice rngui:type='color'>
        <value>#ffffff</value>
        <value>#d4e1f1</value>
        <value>#d8f0dd</value>
        <value>#ffffcc</value>
        <value>#e4c3e4</value>
        <value>#f8a8c6</value>
        <value>#ded0c5</value>
        <value>#deddda</value>
      </choice>
    </element>
    <element name="type" rngui:label="Type">
      <choice>
      </choice>
    </element>
    <element name="model" rngui:label="Model">
      <choice>
      </choice>
    </element> 
    
  </element>
</element>
