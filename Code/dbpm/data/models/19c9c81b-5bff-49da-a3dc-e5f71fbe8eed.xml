<description>
  <dbpm:info xmlns:dbpm="https://example.com/dbpm">
    <dbpm:document_info>
      <dbpm:document_id>7410888c-2812-49ee-93c8-bb3b9b0882ec</dbpm:document_id>
      <dbpm:text_selections>1. Document Ingestion
- User uploads one or more documents (text, PDF, or Word).
- System stores documents and extracts textual c</dbpm:text_selections>
    </dbpm:document_info>
  </dbpm:info>
  <description xmlns="http://cpee.org/ns/description/1.0" svg-id="description" svg-type="description" svg-subtype="description">
    <loop mode="pre_test" condition="data.places.length &gt; 0" svg-id="loop_0" svg-label="data.places.length &gt; 0" svg-type="loop" svg-subtype="loop_head">
      <_probability>
        <_probability_min/>
        <_probability_max/>
        <_probability_avg/>
      </_probability>
      <call id="a1" endpoint="overview" svg-id="a1" svg-label="Move to overview position" svg-type="call" svg-subtype="call">
        <parameters>
          <label>Move to overview position</label>
          <arguments/>
          <stream>
            <sensors/>
            <aggregators/>
            <costs/>
          </stream>
        </parameters>
        <annotations>
          <_timing>
            <_timing_wait/>
            <_timing_threshold/>
            <_timing_min/>
            <_timing_max/>
            <_timing_avg/>
          </_timing>
          <_notes>
            <_notes_general/>
          </_notes>
        </annotations>
        <documentation/>
      </call>
      <call id="a3" endpoint="get_items_base" svg-id="a3" svg-label="Get item positions" svg-type="call" svg-subtype="callmanipulate">
        <parameters>
          <label>Get item positions</label>
          <arguments/>
          <stream>
            <sensors/>
            <aggregators/>
            <costs/>
          </stream>
        </parameters>
        <code>
          <prepare/>
          <finalize output="result">data.items = result</finalize>
          <update output="result"/>
          <rescue output="result"/>
        </code>
        <annotations>
          <_timing>
            <_timing_wait/>
            <_timing_threshold/>
            <_timing_min/>
            <_timing_max/>
            <_timing_avg/>
          </_timing>
          <_notes>
            <_notes_general/>
          </_notes>
        </annotations>
        <documentation/>
      </call>
      <choose mode="exclusive" svg-id="choose_0" svg-type="choose" svg-subtype="choose_exclusive">
        <alternative condition="data.items.length &gt; 0" svg-id="alternative_0" svg-label="data.items.length &gt; 0" svg-type="alternative" svg-subtype="alternative">
          <call id="a5" endpoint="move_camera" svg-id="a5" svg-label="Inspect first item" svg-type="call" svg-subtype="call">
            <parameters>
              <label>Inspect first item</label>
              <arguments>
                <x>!data.items.first['x']</x>
                <y>!data.items.first['y']</y>
              </arguments>
              <stream>
                <sensors/>
                <aggregators/>
                <costs/>
              </stream>
            </parameters>
            <annotations>
              <_timing>
                <_timing_wait/>
                <_timing_threshold/>
                <_timing_min/>
                <_timing_max/>
                <_timing_avg/>
              </_timing>
              <_notes>
                <_notes_general/>
              </_notes>
            </annotations>
            <documentation/>
          </call>
          <call id="a2" endpoint="subprocess" svg-id="a2" svg-label="Adjust" svg-type="call" svg-subtype="callmanipulate">
            <parameters>
              <label>Adjust</label>
              <method>:post</method>
              <arguments>
                <behavior>wait_running</behavior>
                <url>https://cpee.org/flow/templates/UR-VUE%202020%20Manual%20Adjust.xml</url>
                <init/>
                <endpoints/>
                <stream/>
              </arguments>
              <stream>
                <sensors/>
                <aggregators/>
                <costs/>
              </stream>
            </parameters>
            <code>
              <prepare/>
              <finalize output="result">data.place = data.places.shift
data.item = result['item']
data.item['x'] = data.items.first['x'] + data.item['adjust_x']
data.item['y'] = data.items.first['y'] + data.item['adjust_y']</finalize>
              <update output="result"/>
              <rescue output="result"/>
            </code>
            <annotations>
              <_timing>
                <_timing_wait/>
                <_timing_threshold/>
                <_timing_min/>
                <_timing_max/>
                <_timing_avg/>
              </_timing>
              <_notes>
                <_notes_general/>
              </_notes>
            </annotations>
            <documentation/>
          </call>
          <call id="a8" endpoint="grip_and_place" svg-id="a8" svg-label="Grip and Place" svg-type="call" svg-subtype="call">
            <parameters>
              <label>Grip and Place</label>
              <arguments>
                <x>!data.item['x']</x>
                <y>!data.item['y']</y>
                <place_x>!data.place[0]</place_x>
                <place_y>!data.place[1]</place_y>
              </arguments>
              <stream>
                <sensors/>
                <aggregators/>
                <costs/>
              </stream>
            </parameters>
            <annotations>
              <_timing>
                <_timing_wait/>
                <_timing_threshold/>
                <_timing_min/>
                <_timing_max/>
                <_timing_avg/>
              </_timing>
              <_notes>
                <_notes_general/>
              </_notes>
            </annotations>
            <documentation/>
          </call>
          <_probability>
            <_probability_min/>
            <_probability_max/>
            <_probability_avg/>
          </_probability>
        </alternative>
        <otherwise svg-id="otherwise_0" svg-type="otherwise" svg-subtype="otherwise">
          <terminate svg-id="terminate_0" svg-type="terminate" svg-subtype="terminate"/>
        </otherwise>
      </choose>
    </loop>
  </description>
</description>