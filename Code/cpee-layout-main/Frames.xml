      <call id="a5" endpoint="frames_display">
        <parameters>
          <label>Display Graph</label>
          <arguments>
            <type>set</type>
            <lx>3</lx>
            <ly>1</ly>
            <x_amount>2</x_amount>
            <y_amount>7</y_amount>
            <button/>
            <style/>
            <urls>
              <urls_item>
                <lang>de-at</lang>
                <url>!"https://cpee.org/flow/graph.html?monitor=" + cpee.instance_url</url>
              </urls_item>
            </urls>
            <default/>
          </arguments>
        </parameters>
        <annotations>
          <_timing>
            <_timing_weight/>
            <_timing_avg/>
            <_context_data_analysis>
              <probes/>
