        <documentation>
          <input/>
          <output/>
          <implementation>
            <description/>
          </implementation>
        </documentation>
      </call>
      <loop mode="post_test" condition="true">
        <_probability>
          <_probability_min/>
          <_probability_max/>
          <_probability_avg/>
        </_probability>
        <call id="a2" endpoint="frames_display">
          <parameters>
            <label>Show Button</label>
            <arguments>
              <type>wait</type>
              <lx>0</lx>
              <ly>1</ly>
              <x_amount>1</x_amount>
              <y_amount>1</y_amount>
              <button/>
